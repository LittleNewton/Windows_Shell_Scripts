	// Add any keybinding overrides to this array.
	// To unbind a default keybinding, set the command to "unbound"
	"keybindings": [
		{
			// 关闭标签页
			"command": "closeTab",
			"keys": [
				"ctrl+w"
			]
		},
		{
			// 新建默认标签页
			"command": "newTab",
			"keys": [
				"ctrl+t"
			]
		},
		{
			// 新建 1 号标签
			"command": "newTabProfile0",
			"keys": [
				"ctrl+shift+1"
			]
		},
		{
			"command": "newTabProfile1",
			"keys": [
				"ctrl+shift+2"
			]
		},
		{
			"command": "newTabProfile2",
			"keys": [
				"ctrl+shift+3"
			]
		},
		{
			// 切换标签页
			"command": "nextTab",
			"keys": [
				"ctrl+tab"
			]
		},
		{
			// 打开设置文件
			"command": "openSettings",
			"keys": [
				"ctrl+,"
			]
		},
		{
			// 上个标签页
			"command": "prevTab",
			"keys": [
				"ctrl+shift+tab"
			]
		},
		{
			// 向下滚动
			"command": "scrollDown",
			"keys": [
				"ctrl+shift+down"
			]
		},
		{
			// 向下滚动一页
			"command": "scrollDownPage",
			"keys": [
				"ctrl+shift+pgdn"
			]
		},
		{
			// 向上滚动
			"command": "scrollUp",
			"keys": [
				"ctrl+shift+up"
			]
		},
		{
			// 向上滚动一页
			"command": "scrollUpPage",
			"keys": [
				"ctrl+shift+pgup"
			]
		},
		{
			// 切换到首个标签页
			"command": "switchToTab0",
			"keys": [
				"alt+1"
			]
		},
		{
			"command": "switchToTab1",
			"keys": [
				"alt+2"
			]
		},
		{
			"command": "switchToTab2",
			"keys": [
				"alt+3"
			]
		},
		{
			"command": "switchToTab3",
			"keys": [
				"alt+4"
			]
		}
	]
